@ARTICLE{citeulike:673916,
  author = {Liou, Jen and Kiefer, Friedemann and Dang, Alphons and Hashimoto,
	Ari and Cobb, Melanie H. and Kurosaki, Tomohiro and Weiss, Arthur},
  title = {HPK1 Is Activated by Lymphocyte Antigen Receptors and Negatively
	Regulates AP-1},
  journal = {Immunity},
  year = {2000},
  volume = {12},
  pages = {399--408},
  number = {4},
  month = {April},
  abstract = {The serine/threonine kinase HPK1 is a member of the germinal center
	kinase (GCK) family that has been implicated in the regulation of
	MAP kinase pathways. Here, we demonstrate the involvement of HPK1
	in antigen receptor signaling. Engagement of the TCR or the BCR resulted
	in a marked induction of HPK1 catalytic activity. Subsequent analysis
	revealed that Src and Syk/ZAP-70 tyrosine kinases and the adaptor
	proteins LAT, SLP-76, BLNK, Grb2, and Grap are involved in HPK1 activation.
	Overexpression of HPK1 inhibited TCR activation of AP-1 and ERK2,
	whereas the kinase-inactive mutant of HPK1 potentiated these responses.
	Neither form of HPK1 affected PMA or v-Ras activation of AP-1 and
	ERK2. Thus, HPK1 is a negative regulator of the TCR-induced AP-1
	response pathway.},
  citeulike-article-id = {673916},
  doi = {10.1016/S1074-7613(00)80192-2},
  keywords = {hpk1 tcrmr},
  url = {http://www.sciencedirect.com/science/article/B6WSP-41BD5XG-5/2/4221b7315158bbba66b8e3184f0f3fe8}
}

