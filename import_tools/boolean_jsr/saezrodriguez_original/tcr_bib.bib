  title = {{Independent} Human {MAP-kinase} Signal Transduction Pathways Defined
	by {MEK} and {MKK} Isoforms},
  journal = {Science},
  year = {1995},
  volume = {267},
  pages = {682-685},
  abstract = {Mammalian mitogen-activated protein (MAP) kinases include extracellular
	signal-regulated protein kinase (ERK), c-Jun amino-terminal kinase
	(JNK), and p38 subgroups. These MAP kinase isoforms are activated
	by dual phosphorylation on threonine and tyrosine. Two human MAP
	kinase kinases (MKK3 and MKK4) were cloned that phosphorylate and
	activate p38 MAP kinase. These MKK isoforms did not activate the
	ERK subgroup of MAP kinases, but MKK4 did activate JNK. These data
	demonstrate that the activators of p38 (MKK3 and MKK4), JNK (MKK4),
	and ERK (MEK1 and MEK2) define independent MAP kinase signal transduction
	pathways.},
  pmid = {7839144}
}

@ARTICLE{citeulike:555900,
