@ARTICLE{citeulike:419085,
  author = {Lucas, P. C. and Yonezumi, M. and Inohara, N. and McAllister-Lucas,
	L. M. and Abazeed, M. E. and Chen, F. F. and Yamaoka, S. and Seto,
	M. and Nunez, G. },
  title = {Bcl10 and MALT1, independent targets of chromosomal translocation
	in malt lymphoma, cooperate in a novel NF-kappa B signaling pathway.},
  journal = {J Biol Chem},
  year = {2001},
  volume = {276},
  pages = {19012--19019},
  number = {22},
  month = {June},
  abstract = {At least two distinct recurrent chromosomal translocations have been
	implicated in the pathogenesis of MALT lymphoma. The first, t(1;14),
	results in the transfer of the entire Bcl10 gene to chromosome 14
	wherein Bcl10 expression is inappropriately stimulated by the neighboring
	Ig enhancer. The second, t(11;18), results in the synthesis of a
	novel fusion protein, API2-MALT1. Until now, no common mechanism
	of action has been proposed to explain how the products of these
	seemingly unrelated translocations may contribute to the same malignant
	process. We show here that Bcl10 and MALT1 form a strong and specific
	complex within the cell, and that these proteins synergize in the
	activation of NF-kappaB. The data support a mechanism of action whereby
	Bcl10 mediates the oligomerization and activation of the MALT1 caspase-like
	domain. This subsequently activates the IKK complex through an unknown
	mechanism, setting in motion a cascade of events leading to NF-kappaB
	induction. Furthermore, the API2-MALT1 fusion protein also strongly
	activates NF-kappaB and shows dependence upon the same downstream
	signaling factors. We propose a model whereby both the Bcl10.MALT1
	complex and the API2-MALT1 fusion protein activate a common downstream
	signaling pathway that originates with the oligomerization-dependent
	activation of the MALT1 caspase-like domain.},
  address = {Department of Pathology and Comprehensive Cancer Center, Department
	of Pediatrics, The University of Michigan Medical School, Ann Arbor,
	Michigan 48109, USA.},
  citeulike-article-id = {419085},
  comment = {From Thomas MR on 30.11.05 on carma1/card11/bimp3 (Carma1/Bcl10/Malt1
	complex)},
  doi = {10.1074/jbc.M009984200},
  issn = {0021-9258},
  keywords = {carma tcrmr},
  priority = {2},
  url = {http://dx.doi.org/10.1074/jbc.M009984200}
}

