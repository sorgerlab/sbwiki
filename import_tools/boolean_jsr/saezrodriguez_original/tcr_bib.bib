@ARTICLE{citeulike:307317,
  author = {Dan, I. and Watanabe, N. M. and Kusumi, A. },
  title = {The Ste20 group kinases as regulators of MAP kinase cascades},
  journal = {Trends in Cell Biology},
  year = {2001},
  volume = {11},
  pages = {220--230},
  number = {5},
  month = {May},
  abstract = {Ste20p (sterile 20 protein) is a putative yeast mitogen-activated
	protein kinase kinase kinase kinase (MAP4K) involved in the mating
	pathway. Its homologs in mammals, Drosophila, Caenorhabditis elegans
	and other organisms make up a large emerging group of protein kinases
	including 28 members in human. The Ste20 group kinases are further
	divided into the p21-activated kinase (PAK) and germinal center kinase
	(GCK) families. They are characterized by the presence of a conserved
	kinase domain and a noncatalytic region of great structural diversity
	that enables the kinases to interact with various signaling molecules
	and regulatory proteins of the cytoskeleton. This review describes
	the phylogenetic relationships of the Ste20 group kinases based on
	discussions with many researchers in this field. With the newly established
	phylogenetic relationships, crucial arguments can be advanced regarding
	the functions of these kinases as upstream activators of the MAPK
	pathways and possible activity as MAP4Ks. Their involvement in apoptosis,
	morphogenesis and cytoskeletal rearrangements is also discussed.},
  citeulike-article-id = {307317},
  doi = {10.1016/S0962-8924(01)01980-8},
  issn = {0962-8924},
  keywords = {ste20 tcrmr},
  url = {http://www.ingentaconnect.com/content/els/09628924/2001/00000011/00000005/art01980}
}

