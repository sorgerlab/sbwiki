  volume = {153},
  pages = {207-220},
  number = {1},
  abstract = {Diacylglycerol kinase (DGK) is suggested to attenuate diacylglycerol-induced
	cell responses through the phosphorylation of this second messenger
	to phosphatidic acid. Here, we show that DGK{alpha}, an isoform highly
	expressed in T lymphocytes, translocates from cytosol to the plasma
	membrane in response to two different receptors known to elicit T
