@ARTICLE{citeulike:525547,
  author = {Castro, M. A. and Tavares, P. A. and Almeida, M. S. and Nunes, R.
	J. and Wright, M. D. and Mason, D. and Moreira, A. and Carmo, A.
	M. },
  title = {CD2 physically associates with CD5 in rat T lymphocytes with the
