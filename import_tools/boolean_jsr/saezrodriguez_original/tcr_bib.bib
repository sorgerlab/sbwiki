  keywords = {pdk tcrmr},
  url = {http://dx.doi.org/10.1073/pnas.2335486100}
}

@ARTICLE{citeulike:466955,
  author = {Lim, M. A. and Yang, L. and Zheng, Y. and Wu, H. and Dong, L. Q.
